{
    "name": "n8n-dashboard-backend",
    "version": "1.0.0",
    "description": "Backend API server for n8n agent dashboard",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:integration": "jest --testPathPattern=integration",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "db:migrate": "node scripts/migrate.js",
        "db:seed": "node scripts/seed.js"
    },
    "keywords": [
        "n8n",
        "dashboard",
        "api",
        "backend",
        "typescript"
    ],
    "author": "NexDevs",
    "license": "MIT",
    "dependencies": {
        "@neondatabase/serverless": "^0.9.0",
        "@types/ws": "^8.18.1",
        "axios": "^1.13.2",
        "bcrypt": "^5.1.1",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "eventsource": "^2.0.2",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "node-cron": "^3.0.3",
        "sanitize-html": "^2.11.0",
        "winston": "^3.11.0",
        "ws": "^8.18.3",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/eventsource": "^1.1.15",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.6",
        "@types/node-cron": "^3.0.11",
        "@types/sanitize-html": "^2.9.5",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}
